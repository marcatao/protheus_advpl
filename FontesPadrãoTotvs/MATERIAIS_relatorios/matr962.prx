	aDados		:=	Array(4)
	aDados[1]	:=	NBM
	aDados[2]	:=	CODIGO
	aDados[3]	:=	MERCAD
	aDados[4]	:=	Transform(VALOR,cPictVal)
	nTotal		+=	VALOR

	FmtLin(@aDados,aL[7],cPictVal,,@nLin)
	dbSkip()
	nReg++
End
FmtLin(,aL[8],,,@nLin)
FmtLin({nTotal},aL[9],cPictVal,,@nLin)

dbSelectArea(cArqDado)

Return
